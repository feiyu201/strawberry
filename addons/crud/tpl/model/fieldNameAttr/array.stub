public function set{%fieldName%}Attr($value)
{
    if(!is_array($value)){
        throw new \Exception("参数必须是数组");
    }
    return json_encode(array_values($value));
}

public function get{%fieldName%}Attr($value)
{
    try{
        return json_decode($value,true);
    }catch(\Exception $e){
        return [];
    }
   
}